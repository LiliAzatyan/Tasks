class ObjectCreationLimit {
    static #count = 0;
    static #quantity = 3; 

    constructor(name, surname) {
        this.name = name;
        this.surname = surname;
        if (ObjectCreationLimit.#count >= ObjectCreationLimit.#quantity) {
            throw new Error(">Your program has exceeded the specified maximum object creation limit!");
        }
        ObjectCreationLimit.#count++;
    }
};
        
const obj0 = new ObjectCreationLimit("Lili", "Azatyan");
console.log(obj0);
